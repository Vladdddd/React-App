{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "sourceMap": true,
    "rootDir": "./",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react"
  },
  "include": [
    "**/*.ts"
, "src/components/common/Paginator/Paginator.tsx", "src/components/common/formsControls/formsControls.tsx", "src/hoc/withAuthRedirect.tsx", "src/hoc/withAuthRedirect.tsx", "src/App.tsx", "src/App.test.tsx"  ]
}
